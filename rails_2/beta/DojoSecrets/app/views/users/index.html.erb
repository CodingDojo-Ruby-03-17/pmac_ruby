<% if flash[:errors]
    flash[:errors].each do |e| %>
    <p><%= e %></p>
<%  end
   end %>
   <h1>Listing Users</h1>
       <table>
           <thead>
               <tr>
                   <td>First Name</td>
                   <td>Last Name</td>
                   <td>Email</td>
               </tr>
           </thead>
       <tbody>
           <% @user.each do |user| %>
               <tr>
                   <td><%= user.first_name %></td>
                   <td><%= user.last_name %></td>
                   <td><%= user.email %></td>
               </tr>
           <% end %>
       </tbody>
   </table>
   <a href="/users/new">New User</a>






